<?xml version="1.0"?>
<config>
    <modules>
        <Che_Techtalk>
            <version>0.0.1</version>
        </Che_Techtalk>
    </modules>
    <global>
        <models>
            <techtalk>
                <class>Che_Techtalk_Model</class>
            </techtalk>
        </models>

        <blocks>
            <techtalk>
                <class>Che_Techtalk_Block</class>
            </techtalk>
        </blocks>
    </global>

    <frontend>
        <routers>
            <techtalk>
                <use>standard</use>
                <args>
                    <frontName>Techtalk</frontName>
                    <module>Che_Techtalk</module>
                </args>
            </techtalk>
        </routers>

        <layout>
            <updates>
                <test>
                    <file>techtalk.xml</file>
                </test>
            </updates>
        </layout>
    </frontend>
</config>